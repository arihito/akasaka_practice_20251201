    u.username AS ユーザー名,
	p.title AS タイトル,
	p.content AS 本文,
    c.comment AS コメント,
	c.created_at AS コメント日時
FROM
    comments  c
JOIN users  u ON c.user_id = u.user_id
JOIN posts  p ON c.post_id = p.post_id
-- GROUP BY u.user_id
ORDER BY
    u.user_id DESC;</sql><current_tab id="0"/></tab_sql></sqlb_project>
