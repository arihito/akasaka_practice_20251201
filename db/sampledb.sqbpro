E.name AS 社員名,
I.name AS 商品名,
COUNT(*) AS 販売数,
SUM(I.price) AS 売上額
-- S.sales_date AS 取引日,
-- E.mail AS メールアドレス,
-- E.hire_date AS 雇用日,
-- E.salary AS 給与,
-- D.name AS 部署名,
-- D.locale AS 住所
FROM sales S
JOIN item I ON I.item_id = S.item_id
JOIN employee E ON E.employee_id = S.item_id
-- JOIN dept D ON E.dept_id = D.dept_id
-- WHERE S.sales_date BETWEEN '2021-12-10' AND '2021-12-11'
GROUP BY E.employee_id, I.item_id
ORDER BY E.employee_id, I.item_id
;</sql><current_tab id="0"/></tab_sql></sqlb_project>
